                    <xsd:element name="certificateDomainName" type="xsd:string" />
                    <xsd:element name="creationDate" type="xsd:dateTime" />
                    <xsd:element name="certificateType" type="types:certificateTypeWS" />
                    <xsd:element name="contractType" type="types:contractTypeWS" />
                    <xsd:element name="certificateId" type="xsd:int" nillable="true" />
                    <xsd:element name="result" type="xsd:string" nillable="true" />
                    <xsd:element name="resultMessage" type="xsd:string" nillable="true" />
                </xsd:sequence>
            </xsd:complexType>

            <xsd:simpleType name="contractTypeWS">
                <xsd:restriction base="xsd:string">
