                    <xsd:enumeration value="DSS_FINGERPRINT" />
                    <xsd:enumeration value="DSS_FINGERPRINT_ROLLOVER" />
                    <xsd:enumeration value="IDP_MAXTIMEOFFSET" />
                    <xsd:enumeration value="SP_RETURN_URL_PORTAL" />
                    <xsd:enumeration value="CSV_EXPORT_FIELD_SEPARATOR" />
                </xsd:restriction>
            </xsd:simpleType>

