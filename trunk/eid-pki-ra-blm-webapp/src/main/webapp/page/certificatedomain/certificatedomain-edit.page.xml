<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
	no-conversation-view-id="/page/certificatedomain-list.xhtml" 
	login-required="true">
	
	<restrict>#{s:hasRole('ADMIN_USER')}</restrict>
	
	<begin-conversation join="true" />
	
	<action execute="#{certificateDomainHome.wire}" />
	
	<param name="certificateDomainId" value="#{certificateDomainHome.certificateDomainId}" />
	
	<navigation from-action="#{certificateDomainHome.persist}">
		<end-conversation/>
		<redirect view-id="/page/certificatedomain/certificatedomain-list.xhtml" />	
	</navigation>
	
	<navigation from-action="#{certificateDomainHome.update}">
		<end-conversation/>
		<redirect view-id="/page/certificatedomain/certificatedomain-list.xhtml" />	
	</navigation>
	
</page>